    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.2",
    "webpack-dev-middleware": "^5.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.16"
  }
}
